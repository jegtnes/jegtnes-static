<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <title>{{ title }} | Alex Jegtnes</title>
</head>
<body>

  {{>header}}

  <main>

    {{{ contents }}}

    <ul>

      {{#each collections.posts}}
      <li>
        <h3>{{{this.title}}}</h3>
        <article>{{{this.contents}}}</article>
      </li>
      {{/each}}

    </ul>
  </main>

  {{>footer}}

</body>
</html>
